@model Bookish.Models.Member;

@{
    ViewData["Title"] = "Add New Member";
}
<div class="mb-3">
<h1>Add New Member</h1>
</div>
<form asp-action="Add" method="post">
    <div class="mb-3">
    <label asp-for="Name"></label>
    <input asp-for="Name">
    </div>
    <div class="mb-3">
    <label asp-for="Email"></label>
    <input asp-for="Email">
    </div>
    <div class="mb-3">
    <label asp-for="Mobile"></label>
    <input asp-for="Mobile">
    </div>
    <div class="mb-3"></div>
    <label asp-for="DateJoined"></label>
    <input asp-for="DateJoined">
    </div>
    <button type="submit" class="=btn btn-primary">Save Member</button>
</form>